{
  "name": "tav-ui",
  "version": "0.0.0",
  "description": "Tav UI",
  "keywords": [
    "monorepo",
    "vue3",
    "ant-design-vue"
  ],
  "license": "ISC",
  "author": "i7eo",
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    },
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.js",
    "./es/*.mjs": "./es/*.mjs",
    "./es/*": "./es/*.mjs",
    "./lib/*.js": "./lib/*.js",
    "./lib/*": "./lib/*.js",
    "./*": "./*"
  },
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/casta-fe/tav-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "style": "dist/index.css",
  "sideEffects": [
    "dist/*",
    "theme-chalk/*.css",
    "theme-chalk/src/*.less",
    "es/components/*/style/*",
    "lib/components/*/style/*"
  ],
  "scripts": {
    "rmnm": "rimraf node_modules"
  },
  "peerDependencies": {
    "vue": "3.2.21"
  },
  "dependencies": {
    "@ant-design/colors": "6.0.0",
    "@ant-design/icons-vue": "6.0.1",
    "@iconify/iconify": "2.2.1",
    "@purge-icons/generated": "0.7.0",
    "@vueuse/core": "8.6.0",
    "@vueuse/shared": "6.8.0",
    "@zxcvbn-ts/core": "1.0.0",
    "ant-design-vue": "2.2.8",
    "js-pinyin": "0.1.9",
    "lodash-es": "4.17.21",
    "moment": "2.29.3",
    "qrcode": "1.4.4",
    "resize-observer-polyfill": "1.5.1",
    "sortablejs": "1.15.0",
    "vue-types": "4.1.1",
    "vxe-table": "4.2.6",
    "xe-utils": "3.5.4"
  },
  "engines": {
    "node": ">= 16"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
